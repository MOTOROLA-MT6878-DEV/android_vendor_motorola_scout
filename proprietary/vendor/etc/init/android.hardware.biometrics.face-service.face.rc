service biometrics-face-hal /vendor/bin/hw/android.hardware.biometrics.face-service.face-isv
    class hal
    user system
    group camera system

on post-fs-data
    mkdir /data/vendor/.face 0770 system system
    mkdir /data/vendor/tetras_data 0770 system system
    mkdir /data/vendor/tetras_data/debug 0770 system system
    mkdir /data/vendor/tetras_data/config 0770 system system
    mkdir /data/vendor/tetras_data/debug/dumpinfo 0770 system system
    mkdir /data/vendor/tetras_data/facedata 0770 system system
    mkdir /data/vendor/tzstorage 0770 system system
    mkdir /data/vendor/tzstorage/img 0770 system system